{
       "extends": [
              "eslint:recommended",
              "plugin:react/recommended",
              "prettier",
              "plugin:prettier/recommended"
        ],
       "plugins": [
              "react",
              "prettier"
        ],
       "parserOptions": {
              "ecmaVersion": 6,
              "sourceType": "module",
              "ecmaFeatures": {
                   "jsx": true
              }
        },
       "env": {
              "browser": true,
              "node": true
        },
       "settings": {
              "import/resolver": {
                   "node": {
                          "paths": [
                                  "src"
                          ]
                    }
              }
        }
}